<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="geomex.security.mapper.UserMapper">

	<select id="findUser" parameterType="int" resultType="shopping.model.UserVO">
		SELECT *
		FROM shopping.user
		WHERE  = #{userId}
	</select>

</mapper> 
